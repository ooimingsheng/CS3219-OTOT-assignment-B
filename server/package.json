{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.10",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/supertest": "^2.0.10",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "global": "^4.4.0",
    "http-status-codes": "^1.4.0",
    "jest": "^26.4.2",
    "pg": "^8.3.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.59",
    "typeorm": "^0.2.25",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "lint": "prettier --write \"src/**/*.{ts,tsx}\" --tab-width 2",
    "start": "set debug=* && ts-node-dev ./src/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js",
    "test": "jest",
    "test:circleci": "cross-env NODE_ENV=circleci jest",
    "build": "tsc && cp package.json ./build/ && cd build && yarn install",
    "production": "cd build && cross-env NODE_ENV=production node ./src/index.js",
    "typeorm:run": "npm run typeorm migration:run"
  }
}
