{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/node": "^14.6.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "pg": "^8.3.2",
    "prettier": "^2.0.5",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.59",
    "typeorm": "^0.2.25",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "lint": "prettier --write \"src/**/*.{ts,tsx}\" --tab-width 2",
    "start": "set debug=* && ts-node-dev ./src/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js",
    "test": "jest --runInBand",
    "test:circleci": "cross-env NODE_ENV=circleci jest --runInBand",
    "build": "tsc && cp package.json ./build/ && cd build && yarn install",
    "production": "cd build && cross-env NODE_ENV=production node ./src/index.js",
    "typeorm:run": "npm run typeorm migration:run"
  }
}
